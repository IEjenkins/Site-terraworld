            .title:after {
                content: '';
                display: block;
                position: absolute;
                margin-top: 5px;
                border-radius: 6px;
                width: 150px;
                height: 5px;
                background: #fa5252;
                box-shadow: 0px -2px 12px #fa5252;
            }

            .category {
                background-color: #fa525240;
                color: #fa5252;
            }
            .category.active {
                background-color: #fa5252;
                color: white;
            }
            .category > svg > path {
                stroke: #fa5252
            }
            .category.active > svg > path {
                stroke: white;
            }

            .select-server-card.active {
