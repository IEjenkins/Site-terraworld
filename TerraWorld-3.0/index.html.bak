            }

            .cart-card:active {
                background-color: #fa5252c1;
            }
            .cart-card:hover > .card-body > .badge-icon {
                background: white !important;
            }
            .cart-card:hover > .card-body > .badge-icon > svg > path {
                stroke: #fa5252c1;
            }

            .btn-plus, .btn-minus {
                background: #fa525226;
                border-radius: 13px;
                padding: 14px 14px;
            }
            .btn-plus:hover, .btn-minus:hover {
                background: #fa5252;
            }
            .btn-minus {
                width: 50px;
                height: 50px;
                border-radius: 0;
            }

            input:focus:not(.search) {
                border: 2px solid #fa5252 !important;
            }
